xkb_keymap {
xkb_keycodes {
	minimum = 1;
	maximum = 255;
	<>                   = 1;
};

xkb_types {
	type "ONE_LEVEL" {
		modifiers= none;
	};
};

xkb_compatibility {
	interpret.useModMapMods= AnyLevel;
	interpret.repeat= False;
	interpret VoidSymbol+AnyOfOrNone(none) {
		action= NoAction();
	};
};

xkb_symbols {
	key <>                   {
		repeat= No,
		symbols[1]= [                     VoidSymbol ],
		actions[1]= [        SetMods(modifiers=none) ]
	};
};

};
